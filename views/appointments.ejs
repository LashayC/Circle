							<section>
								<h2>Add Appointment</h2>
								<form method="post" action="/addAppointments">
									<div class="row gtr-uniform">
										
										<h4 class="col-12 col-12-small">Appointment Date</h4>
										<div class="col-12 col-12-xsmall">
											<input type="date" name="date"  placeholder="Date" />
										</div>
										
										<h4 class="col-12 col-12-small">Time</h4>
										<div class="col-12 col-12-xsmall">
											<input type="time" name="time"  placeholder="Time" />
										</div>

										<div class="col-12 col-12-xsmall">
											<input type="text" name="location"  placeholder="Location" />
										</div>

										<div class="col-12 col-12-xsmall">
											<input type="text" name="appointmentType"  placeholder="Appointment Type" />
										</div>

										<div class="col-12 col-12-xsmall">
											<input type="text" name="providerName"  placeholder="Provider Name" />
										</div>

										<div class="col-12">
											<textarea name="reasonNotes" placeholder="Reason for visit" rows="6"></textarea>
										</div>
									
										<div class="col-12">
											<ul class="actions">
												<li><input type="submit" class="primary" /></li>
												<li><input type="reset" value="Reset" /></li>
											</ul>
										</div>
									</div>
								</form>
